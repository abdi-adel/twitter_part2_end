<div class="d-flex flex-row content mx-auto">
    <div class="profile-container"> 
        <%- include('../includes/profile', {user : 'user1'}); %>
    </div>
    <div class="flex-fill d-flex flex-column"> 
        <% if(tweets) { 
            tweets.forEach(function(tweet) { %>
                <div class="card tweet-element">
                    <div class="card-body">
                        <%= tweet.content %>
                    </div>
                </div>
            <% }) %>
        <% } %>
    </div>
</div>